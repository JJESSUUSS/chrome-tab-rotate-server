{
  "name": "chrome-tab-rotate-server",
  "version": "1.1.5",
  "description": "An API that provides the config/contents for the chrome-tab-rotate Chrome extension",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "start": "node src/index",
    "pretest": "eslint ./src",
    "test": "./node_modules/.bin/mocha --reporter spec",
    "test:coverage": "./node_modules/.bin/nyc npm test",
    "test:functional": "./node_modules/.bin/mocha --reporter spec ./test/functional",
    "test:all": "./node_modules/.bin/run-s test:functional test:coverage",
    "docker:status": "node src/healthcheck",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timoa/chrome-tab-rotate-server.git"
  },
  "keywords": [
    "chrome",
    "chrome-tabs",
    "chrome-extension",
    "api",
    "server",
    "config"
  ],
  "author": {
    "name": "Damien Laureaux",
    "email": "d.laureaux@timoa.com",
    "url": "https://timoa.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timoa/chrome-tab-rotate-server/issues"
  },
  "homepage": "https://github.com/timoa/chrome-tab-rotate-server#readme",
  "dependencies": {
    "boom": "7.3.0",
    "fastify": "2.15.3",
    "fastify-healthcheck": "2.3.1",
    "fastify-swagger": "4.17.1",
    "http": "0.0.0",
    "lodash": "4.17.21",
    "snyk": "1.885.0",
    "uuid": "7.0.3",
    "winston": "3.6.0"
  },
  "engines": {
    "node": ">=10.0"
  },
  "os": [
    "linux",
    "win32",
    "darwin"
  ],
  "devDependencies": {
    "acorn": "6.4.2",
    "acorn-jsx": "5.3.2",
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "espree": "4.1.0",
    "mocha": "9.2.2",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "request": "2.88.2",
    "semantic-release": "19.0.2"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "include": [
      "src/*.js",
      "src/**/*.js"
    ],
    "exclude": [
      "test/*.spec.js",
      "coverage/**",
      "src/swagger/*.js",
      "data/**/*"
    ],
    "all": true
  },
  "snyk": true
}
